      <argumentList>
        <argument>
          <name>Identity</name> 
          <direction>in</direction> 
          <relatedStateVariable>A_ARG_TYPE_string</relatedStateVariable> 
        </argument>
        <argument>
          <name>RoleList</name> 
          <direction>in</direction> 
          <relatedStateVariable>A_ARG_TYPE_string</relatedStateVariable> 
        </argument>       
      </argumentList>
