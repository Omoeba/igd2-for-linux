      </argumentList>
    </action>
    <action>
      <name>AddLoginData</name> 
      <argumentList>
         <argument>
          <name>Algorithm</name> 
          <direction>in</direction> 
          <relatedStateVariable>A_ARG_TYPE_string</relatedStateVariable> 
        </argument>
        <argument>
          <name>Flags</name> 
