          <direction>in</direction> 
          <relatedStateVariable>A_ARG_TYPE_string</relatedStateVariable> 
        </argument>
      </argumentList>
    </action>  
    <action>
      <name>AddIdentityData</name>
      <argumentList> 
        <argument>
          <name>IdentityList</name> 
          <direction>in</direction> 
          <relatedStateVariable>A_ARG_TYPE_string </relatedStateVariable> 
