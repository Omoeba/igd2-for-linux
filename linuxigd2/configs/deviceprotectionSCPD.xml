        </argument>
        <argument>
          <name>NewInMessage</name> 
          <direction>in</direction> 
          <relatedStateVariable>A_ARG_TYPE_base64</relatedStateVariable> 
        </argument>
        <argument>
          <name>NewOutMessage</name> 
          <direction>out</direction> 
          <relatedStateVariable>A_ARG_TYPE_base64</relatedStateVariable> 
        </argument>        
      </argumentList>
    </action>
     <action>
