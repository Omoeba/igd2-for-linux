      <argumentList>
        <argument>
          <name>Algorithm</name> 
          <direction>in</direction> 
          <relatedStateVariable>A_ARG_TYPE_string</relatedStateVariable> 
        </argument>
        <argument>
          <name>Name</name> 
          <direction>in</direction> 
          <relatedStateVariable>A_ARG_TYPE_string</relatedStateVariable> 
        </argument>
        <argument>
          <name>Authenticator</name> 
          <direction>in</direction> 
          <relatedStateVariable>A_ARG_TYPE_string</relatedStateVariable> 
        </argument>
      </argumentList>
    </action> 
